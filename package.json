{
  "name": "lost-pet-api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "sync:db": "ts-node ./server/sync.ts",
    "dev:fe": "parcel serve ./public/index.html",
    "dev:be": "nodemon --watch './server/**/*' -e ts --exec 'ts-node ./server/index.ts'",
    "build:be": "tsc",
    "build:fe": "parcel build ./public/index.html --dist-dir ./dist",
    "build": "yarn run build:fe && yarn run build:be",
    "start": "node ./server-dist/index.js"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "parcel": "^2.6.2"
  },
  "dependencies": {
    "@types/node": "^18.0.1",
    "algoliasearch": "^4.13.1",
    "cloudinary": "^1.30.1",
    "dotenv": "^16.0.1",
    "dropzone": "^6.0.0-beta.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mapbox-gl": "^2.9.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.0",
    "tsc": "^2.0.4",
    "typescript": "^4.7.4"
  }
}
